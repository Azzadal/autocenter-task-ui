{
  "printWidth": 90,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ["*.ts", ".tsx"],
      "options": {
        "parser": "babel-ts"
      }
    }
  ]
}
